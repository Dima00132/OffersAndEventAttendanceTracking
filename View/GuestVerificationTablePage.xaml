<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ScannerAndDistributionOfQRCodes.View.GuestVerificationTablePage"
             xmlns:viewModel="clr-namespace:ScannerAndDistributionOfQRCodes.ViewModel"
             NavigationPage.HasNavigationBar="False"
             xmlns:view="clr-namespace:ScannerAndDistributionOfQRCodes.View"
             x:DataType="viewModel:GuestVerificationTableViewModel">

    <view:ScannerQRCodePage IconImageSource="scanner_qr_code.png"  BindingContext="{Binding ScannerQRCodeViewModel}" Title="Сканер"/>
    <view:GuestListPage IconImageSource="guest_list.png" BindingContext="{Binding GuestListViewModel}" Title="Список гостей"/>

</TabbedPage>